{
  "name": "blog",
  "version": "1.0.0",
  "description": "My blog",
  "scripts": {
    "clean": "rm -rf public src/css/styles.css",
    "dev": "npm run clean && postcss src/css/tailwind.css > src/css/styles.css && eleventy --serve",
